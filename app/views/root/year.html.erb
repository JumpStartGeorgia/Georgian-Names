<h1>
  Birth Year: <%= params[:id] %>
</h1>

<div class="row">
  <div class="span4">
    <h3><%= t('app.common.first_name')%></h3>
    <ol class="standard">
    <% @year_first_names.each do |year| %>
      <li>
        <%= link_to year.name.name, first_name_path(year.name.name) %><%= ": #{number_with_delimiter(year.count)}" %>
      </li>
    <% end %>
    </ol>
    <p>
    Total Unique First Names: <%= number_with_delimiter(@total_first.count)%>
    </p>
   </div> 
  <div class="span4">
    <h3><%= t('app.common.last_name')%></h3>
    <ol class="standard">
    <% @year_last_names.each do |year| %>
      <li>
        <%= link_to year.name.name, last_name_path(year.name.name) %><%= ": #{number_with_delimiter(year.count)}" %>
      </li>
    <% end %>
    </ol>
    <p>
    Total Unique Last Names: <%= number_with_delimiter(@total_last.count)%>
    </p>
   </div>  
</div>   